#!groovy

pipeline {
    agent { label 'master' }
    options {
        ansiColor('xterm')
        disableConcurrentBuilds()
        timestamps()
    }
    stages {
        stage('First Step') {
            steps {
                echo "Building image"
                dir ('docker/toolbox')
                {
                    sh 'docker build . '
                }
            }
        }        
    }
}
